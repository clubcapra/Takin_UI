{
  "name": "capra_web_ui",
  "version": "2.2.12",
  "author": {
    "name": "Club Capra",
    "email": "admin@clubcapra.com",
    "url": "http://www.clubcapra.com"
  },
  "description": "User Interface for ROS based robots",
  "private": true,
  "scripts": {
    "start": "electron-snowpack dev",
    "build": "npm run build:snowpack && npm run build:electron",
    "build:snowpack": "electron-snowpack build",
    "build:electron": "electron-builder --publish never",
    "test": "jest",
    "lint": "eslint --ext js,ts,tsx src",
    "fix": "eslint --fix --ext js,ts,tsx src",
    "typecheck": "tsc --noEmit",
    "clean": "electron-snowpack clean",
    "postinstall": "patch-package",
    "local_ci": "npm run lint && npm run typecheck && npm test"
  },
  "build": {
    "appId": "com.clubcapra.capra_web_ui",
    "linux": {
      "target": [
        "deb"
      ]
    },
    "artifactName": "capra_web_ui_setup.${ext}",
    "extends": "electron-snowpack/config/electron-builder.js"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.1",
    "@xstate/react": "^1.6.1",
    "chart.js": "^3.5.1",
    "chartjs-adapter-date-fns": "^2.0.0",
    "date-fns": "^2.24.0",
    "dotenv": "^10.0.0",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.25",
    "polished": "^4.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "react-toastify": "^8.0.2",
    "ros3d": "^1.0.1",
    "roslib": "^1.1.0",
    "styled-components": "^5.3.1",
    "xstate": "^4.25.0"
  },
  "devDependencies": {
    "@snowpack/plugin-react-refresh": "^2.5.0",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.173",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/react-router-dom": "^5.1.9",
    "@types/roslib": "^1.1.3",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "electron": "^14.0.1",
    "electron-builder": "^22.11.7",
    "electron-devtools-installer": "^3.2.0",
    "electron-snowpack": "^0.12.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.2",
    "jest": "^27.2.0",
    "patch-package": "^6.4.7",
    "prettier": "^2.4.1",
    "snowpack": "^3.8.8",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  }
}
